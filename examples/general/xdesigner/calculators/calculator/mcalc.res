! Defaults file for the Motif calculator.
! Jeremy Huxtable 1990

! Display formats and error messages.
MCalc.errorString: Error
MCalc.overString: Stack overflow
MCalc.underString: Stack underflow
MCalc.infinityString: Infinity
MCalc.displayFormat: %19g
MCalc.stringFormat: %20s
MCalc.reversePolish: false
MCalc.showPendingOp: true

! Cosmetic stuff
MCalc*background: #e5e5e5e5e5e5
MCalc*XmPushButton*background: #b0b0c4c4dede
MCalc*XmText*background: #ceb8e75be75b
MCalc*XmMenuShell*background: #cccccccccccc
MCalc*logo_l.labelPixmap: mcalc.bm
MCalc*fontList: -adobe-helvetica-medium-r-normal--12-120-75-75-p-67-iso8859-1

! Note: the display must use a fixed width font as the right-justification
! is done by padding to the left with spaces.
MCalc*calc_display.fontList: -adobe-courier-bold-r-normal--12-120-75-75-m-70-iso8859-1
